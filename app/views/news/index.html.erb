<div class = "news">
 <h1>#BlackTwitter Headline News</h1>
  <% @tweet_news.each do |tweet| %>
   <div class = "row">
    <div class = "col-lg-4">
     <blockquote class="twitter-tweet"
      data-link-color="#55acee" lang="en">
      <p><%= tweet.content %></p>
      <a href= "<%= tweet.tweet_url %>"></a>
    </blockquote>
  <% end %>
  </div>
  </div>

  <h1>#BlackTwitter Updates</h1>
   <% @tweet_updates.each do |tweet| %>
    <div class = "row">
     <div class = "col-lg-4">
      <blockquote class="twitter-tweet"
       data-link-color="#55acee" lang="en">
       <p><%= tweet.content %></p>
       <a href= "<%= tweet.tweet_url %>"></a>
     </blockquote>
   <% end %>
   </div>
   </div>


 </div>
