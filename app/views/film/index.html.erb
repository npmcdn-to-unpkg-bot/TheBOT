<div class = "news">
 <h1>#BlackTwitter Film</h1>
  <% @tweet_films.each do |tweet| %>
   <div class = "row">
    <div class = "col-lg-4">
     <blockquote class="twitter-tweet"
      data-link-color="#55acee" lang="en">
      <p><%= tweet.content %></p>
      <a href= "<%= tweet.tweet_url %>"></a>
    </blockquote>
  <% end %>
 </div>
</div>
