
<div id="example" style="width: 550px; height: 350px;"></div>

<div class = "news">
 <h1>#BlackTwitter Music</h1>
  <% @tweet_music.in_groups_of(4, false).each do |group| %>
   <div class = "row">
    <% group.each do |tweet| %>
      <div class = "col-lg-3">
       <blockquote class="twitter-tweet"
        data-link-color="#55acee" lang="en">
        <p><%= tweet.content %></p>
        <a href= "<%= tweet.tweet_url %>"></a>
       </blockquote>
      </div>
    <% end %>
   </div>
  <% end %>
</div>
